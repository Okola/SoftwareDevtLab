<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".PreviousPregnancy"
    android:id="@+id/FrameLayout"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PREVIOUS PREGNANCY"
            android:textColor="@color/primary"
            android:layout_gravity="center_horizontal"
            android:layout_marginVertical="5dp"
            android:textSize="18sp"/>
        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scrollbars="none"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="12dp">
                    <TableRow>
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:textColor="@color/primary"
                            android:text="PREGNANCY ORDER"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:textColor="@color/primary"
                            android:text="Year"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:textColor="@color/primary"
                            android:text="Number of TImes ANC Attended"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:textColor="@color/primary"
                            android:text="Place Of Delivery"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:textColor="@color/primary"
                            android:text="Maturity"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:id="@+id/labourDuration"
                            android:textColor="@color/primary"
                            android:text="Duration of Labour"
                            android:textSize="15sp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:textColor="@color/primary"
                            android:text="Type of Delivery"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:textColor="@color/primary"
                            android:text="Birth Weight in Kg"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:textColor="@color/primary"
                            android:text="Sex"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:gravity="center_horizontal"
                            android:textColor="@color/primary"
                            android:text="Outcome"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                        <TextView
                            android:background="@drawable/cell_shape"
                            android:layout_width="100dp"
                            android:layout_height="40dp"
                            android:textColor="@color/primary"
                            android:gravity="center_horizontal"
                            android:text="Prueperium"
                            android:textSize="15sp"
                            tools:ignore="SmallSp" />
                    </TableRow>
                </TableLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/previousPregnancyRecycler"/>

            </LinearLayout>


        </HorizontalScrollView>








        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="@string/update_details"
            android:textSize="12sp"
            android:layout_marginTop="5dp"
            android:id="@+id/updatematernalprofile"/>



    </LinearLayout>




</FrameLayout>