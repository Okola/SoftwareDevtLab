<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/secondary"
    android:backgroundTint="@color/secondary"
    android:orientation="vertical">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginBottom="50dp">

            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/childParticularsCard"
                app:cardElevation="14sp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PARTICULARS OF CHILD"
                    android:textSize="18sp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                   
                    android:drawablePadding="18dp"
                    tools:ignore="RtlSymmetry" />

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/healthRecord"
                app:cardElevation="14sp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="HEALTH RECORD OF CHILD"
                    android:textSize="18sp"
                    android:paddingTop="1dp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                   
                    android:drawablePadding="18dp"
                    tools:ignore="RtlSymmetry" />

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/civilRegistration"
                app:cardElevation="14sp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="CIVIL REGISTRATION"
                    android:textSize="18sp"
                    android:paddingTop="1dp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                   
                    android:drawablePadding="18dp"
                    tools:ignore="RtlSymmetry" />

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"

                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/clinicalReview"
                app:cardElevation="14sp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="CLINICAL REVIEW"
                    android:textSize="18sp"
                    android:paddingTop="1dp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                   
                    android:drawablePadding="18dp"
                    tools:ignore="RtlSymmetry" />

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"

                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/feedingInfo"
                app:cardElevation="14sp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FEEDING INFORMATION"
                    android:textSize="18sp"
                   android:paddingTop="1dp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                   
                    android:drawablePadding="18dp"
                    tools:ignore="RtlSymmetry" />

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"

                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/behaviouralCharacteristics"
                app:cardElevation="14sp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="CHARACTERISTICS"
                    android:textSize="18sp"
                   android:paddingTop="1dp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                   
                    android:drawablePadding="6dp"
                    tools:ignore="RtlSymmetry" />

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"

                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/developmentMilestones"
                app:cardElevation="14sp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="DEVELOPMENT MILESTONES"
                    android:textSize="18sp"
                   android:paddingTop="1dp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                   
                    android:drawablePadding="18dp"
                    tools:ignore="RtlSymmetry" />

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/developmentprogressWeight"
                app:cardElevation="14sp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="WEIGHT PROGRESS"
                    android:textSize="18sp"
                   android:paddingTop="1dp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                   
                    android:drawablePadding="18dp"
                    tools:ignore="RtlSymmetry" />



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/developmentprogressHeight"
                app:cardElevation="14sp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="HEIGHT PROGRESS"
                    android:textSize="18sp"
                   android:paddingTop="1dp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                   
                    android:drawablePadding="18dp"
                    tools:ignore="RtlSymmetry" />



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="300dp"
                android:layout_height="wrap_content"

                android:backgroundTint="@color/primary"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                app:cardCornerRadius="10dp"
                android:id="@+id/immunization"
                android:layout_marginBottom="50dp"
                app:cardElevation="14sp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="IMMUNIZATION"
                    android:textSize="18sp"
                   android:paddingTop="1dp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/secondary"
                    android:layout_marginVertical="5dp"
                    android:paddingStart="15dp"
                    android:drawablePadding="18dp"
                    tools:ignore="RtlSymmetry" />

            </androidx.cardview.widget.CardView>


        </LinearLayout>

    </ScrollView>


</androidx.appcompat.widget.LinearLayoutCompat>