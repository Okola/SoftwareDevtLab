<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/secondary"
        android:backgroundTint="@color/secondary"
        android:id="@+id/card_doctor"
        android:layout_margin="1dp">


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="PREVIOUS PREGNANCY"
            android:textSize="18sp"
            android:textColor="@color/secondary"
            android:layout_gravity="center_horizontal"
            android:gravity="center_horizontal"
            android:backgroundTint="@color/primary"
            android:background="@color/primary"
            android:layout_marginBottom="5dp"/>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginVertical="16dp"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Pregnancy Order"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/pregnancyOrder1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Year"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:id="@+id/year1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Number of Times ANC attended"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/ancTimes1"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Place of Delivery"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/placeofdelivery1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Maturity"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/maturity1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Duration of labour"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/labourDuration1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Type of Delivery"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/typeofdelivery1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Birth Weight in Kgs"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/birthweight1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Sex"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/sex1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Outcome"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/outcome1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Puerperium"
                    android:paddingStart="12dp"
                    android:textColor="@color/primary"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
                <EditText
                    android:id="@+id/prueperium1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text="SUBMIT"
                android:textSize="15sp"
                android:id="@+id/submit"
                android:textColor="@color/secondary"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:layout_marginBottom="15dp"
                android:background="@drawable/round_btn"/>



        </LinearLayout>

    </androidx.cardview.widget.CardView>

</LinearLayout>